# 포트 설정
# HTTP 서버 포트 (기본값: 8888)
HTTP_PORT=8888

# WebSocket 서버 포트 (기본값: 8765)
WS_PORT=8765

# 간단 로그인 비밀번호 (선택)
# 빈 값이면 로그인 없이 사용 가능
APP_LOGIN_PASSWORD=

# JWT 서명 키 (로그인 사용 시 필수)
APP_JWT_SECRET=
# JWT 만료 시간(초), 기본 7일
APP_JWT_TTL=604800

# AI 제공자 API 키 (컨테이너 환경에서 필요)
# Factory AI (Droid) API 키
# https://app.factory.ai/settings/api-keys 에서 발급 가능
FACTORY_API_KEY=your_factory_api_key_here
